{
  "buildCommand": "pnpm install && pnpm build",
  "installCommand": "pnpm install",
  "framework": "other",
  "outputDirectory": ".vinxi/dist/public",
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/trpc/(.*)",
      "destination": "/api/trpc"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

