{
  "version": 2,
  "buildCommand": "./build.sh",
  "devCommand": "pnpm dev",
  "installCommand": "pnpm install",
  "framework": null,
  "name": "slumberland",
  "outputDirectory": ".vinxi/dist/public",
  "cleanUrls": true,
  "trailingSlash": false,
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

