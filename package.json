{
  "name": "slumberland-monorepo",
  "version": "1.0.0",
  "description": "Slumberland - AI-Powered Influencer Marketing Discovery Platform",
  "private": true,
  "engines": {
    "node": ">=16.x",
    "npm": ">=8.x"
  },
  "scripts": {
    "start": "cd server && npm start",
    "install:server": "cd server && npm install",
    "install:client": "cd slumberland-ui && npm install",
    "install:all": "npm run install:server && npm run install:client",
    "dev:server": "cd server && npm start",
    "dev:client": "cd slumberland-ui && npm start",
    "build:client": "cd slumberland-ui && npm run build"
  },
  "keywords": ["slumberland", "influencer", "marketing", "ai"],
  "author": "",
  "license": "MIT"
}
